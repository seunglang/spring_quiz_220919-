<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
 <mapper namespace="com.quiz.lesson03.dao.RealEstateDAO">
 	
 	<select id="selectRealEstateId" parameterType="int" resultType="com.quiz.lesson03.model.RealEstate">
 		SELECT
 			`id`
			,`realtorId`
			,`address`
			,`area`
			,`type`
			,`price`
			,`rentPrice`
			,`createdAt`
			,`updatedAt`
		FROM
			`real_estate`
		WHERE
			
			`id` = #{id}
 	</select>
 
 	<select id="selectRealEstateRentPrice" parameterType="int" resultType="map">
 		SELECT
 			`id`
			,`realtorId`
			,`address`
			,`area`
			,`type`
			,`price`
			,`rentPrice`
			,`createdAt`
			,`updatedAt`
		FROM
			`real_estate`
		WHERE
			
			rentPrice &lt; #{value} 
 	</select>
 	
 	<select id="selectRealEstateAP" parameterType="int" resultType="map">
 		SELECT
 			`id`
			,`realtorId`
			,`address`
			,`area`
			,`type`
			,`price`
			,`rentPrice`
			,`createdAt`
			,`updatedAt`
		FROM
			`real_estate`
		WHERE
			
			 <![CDATA[
        		area > 90 AND price <= 130000
        	]]>
        	
        	캡쳐용
        	area &gt; #{area} AND price &lt; #{price}
		ORDER BY
		
		`updatedAt` desc
 	</select>
 </mapper>